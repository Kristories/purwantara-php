{
    "name": "purwantaraid/purwantara-php",
    "description": "PHP Wrapper for Purwantara API.",
    "keywords": [
        "purwantara",
        "purwantara-php"
    ],
    "homepage": "https://github.com/purwantaraid/purwantara-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Wahyu Kristianto",
            "email": "wahyu@purwantara.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^2.15",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Purwantara\\Purwantara\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Purwantara\\Purwantara\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
